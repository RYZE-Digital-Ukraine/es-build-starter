<!DOCTYPE html>
<html lang="{{ lang }}">

	{% include "../parts/head.twig" %}

	<body>

    <!-- Sprite :: Start -->
    <div id="sprite" style="width: 0; height: 0; visibility: hidden;" data-path="{{ baseurl }}"></div>
    <!-- Sprite :: End -->

    <!-- Header :: Start -->
    {% include "../parts/header.twig" %}
    <!-- Header :: End -->

    <!-- Main :: Start -->
    <main class="wrapper min-vh-100">
		  {% block content %}{% endblock %}
      {% block footer %}{% endblock %}
    </main>
    <!-- Main :: End -->

    <!-- Scripts :: Start -->
		{% for item in externalscripts %}
			<script type="text/javascript" src="{{ item }}"></script>
		{% endfor %}

		<script src="./js/index.js"></script>
    <!-- Scripts :: End -->

	</body>
</html>
